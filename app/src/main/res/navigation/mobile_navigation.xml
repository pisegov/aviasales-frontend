<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/main_screen_navigation">

    <include app:graph="@navigation/main_screen_navigation" />

    <include app:graph="@navigation/search_navigation" />

    <include app:graph="@navigation/selected_country_search_navigation" />

    <include app:graph="@navigation/tickets_navigation" />

    <fragment
        android:id="@+id/navigation_hotels"
        android:name="com.myaxa.common.placeholder.PlaceholderFragment"
        android:label="@string/title_hotels"
        tools:layout="@layout/fragment_placeholder" />

    <fragment
        android:id="@+id/navigation_short"
        android:name="com.myaxa.common.placeholder.PlaceholderFragment"
        android:label="@string/title_short"
        tools:layout="@layout/fragment_placeholder" />

    <fragment
        android:id="@+id/navigation_subscriptions"
        android:name="com.myaxa.common.placeholder.PlaceholderFragment"
        android:label="@string/title_subscriptions"
        tools:layout="@layout/fragment_placeholder" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.myaxa.common.placeholder.PlaceholderFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_placeholder" />

</navigation>